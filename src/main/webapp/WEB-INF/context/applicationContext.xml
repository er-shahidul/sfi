		<constructor-arg>
            <util:properties location="classpath*:errors/cs3"/>
        </constructor-arg>
	</bean>

	<bean id="cs4Errors" class="java.util.HashMap">
		<constructor-arg>
            <util:properties location="classpath*:errors/cs4"/>
        </constructor-arg>
	</bean>

	<bean id="cs5Errors" class="java.util.HashMap">
		<constructor-arg>
            <util:properties location="classpath*:errors/cs5"/>
        </constructor-arg>
	</bean>

	<bean id="cs6Errors" class="java.util.HashMap">
